From bb769e682010ef55ac32713d0a90dc84316f3522 Mon Sep 17 00:00:00 2001
From: David Eddlemon <DavidEddlemon@Hotmail.com>
Date: Tue, 20 Aug 2013 00:25:59 +0400
Subject: [PATCH] Strict Aliasing Fix

---
 Android.mk             | 2 ++
 src/jni/rtp/Android.mk | 2 +-
 2 files changed, 3 insertions(+), 1 deletion(-)

diff --git a/Android.mk b/Android.mk
index 35eaad2..f1bbd5d 100644
--- a/Android.mk
+++ b/Android.mk
@@ -12,6 +12,8 @@
 # See the License for the specific language governing permissions and
 # limitations under the License.
 
+LOCAL_CFLAGS += -fno-strict-aliasing
+
 LOCAL_PATH := $(call my-dir)
 
 include $(CLEAR_VARS)
diff --git a/src/jni/rtp/Android.mk b/src/jni/rtp/Android.mk
index 15b8587..8ab9d44 100644
--- a/src/jni/rtp/Android.mk
+++ b/src/jni/rtp/Android.mk
@@ -53,7 +53,7 @@ LOCAL_C_INCLUDES += \
 	frameworks/av/media/libstagefright/codecs/amrnb/dec/src \
 	$(call include-path-for, audio-effects)
 
-LOCAL_CFLAGS += -fvisibility=hidden
+LOCAL_CFLAGS += -fvisibility=hidden -fno-strict-aliasing
 
 
 
-- 
